{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/middlewares/(.*)",
        "dest": "/middlewares/$1"
      },
      {
        "src": "/model/(.*)",
        "dest": "/model/$1"
      },
      {
        "src": "/routes/(.*)",
        "dest": "/routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  